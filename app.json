{
  "expo": {
    "name": "protocol_notes",
    "slug": "protocolNotes",
    "owner": "Gregory Boyd",
    "version": "1.0.0",
    "platforms": ["ios", "android"],
    "orientation": "portrait",
    "icon": "./assets/images/walk-the-pet.png",
    "scheme": "basic",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/pawprint.png",
      "resizeMode": "contain",
      "backgroundColor": "#F6F4F3s"
    },
    "ios": {
      "supportsTablet": true
      //"packages": "com.petswithoutnames.protocols"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": ""
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "../assets/fonts/Ubanist-Regular",
            "../assets/fonts/Urbanist-Medium",
            "../assets/fonts/ConcertOne-Regular"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
